{
  "name": "ng-example-ngrx",
  "version": "0.0.0",
  "author": "Merih Sakarya <merihsakarya@yahoo.com> (https://linkedin.com/in/merih-sakarya)",
  "description": "Angular Starter Template based on NGRX + Material Design",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:test": "ng build --configuration=${NG_ENV:-test}",
    "build:prod": "ng build --configuration=${NG_ENV:-production}",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:unit & npm run test:e2e",
    "test:ci": "npm run test:unit:ci & npm run test:e2e:ci",
    "test:unit": "ng test",
    "test:unit:ci": "ng test --karma-config=karma-ci.conf.js --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:e2e": "start-server-and-test start 4200 test:server 3000 cypress:open",
    "test:e2e:ci": "start-server-and-test start 4200 test:server:ci 3000 cypress:run",
    "test:server": "nodemon server/server.js --ext js,ts,json,html",
    "test:server:ci": "node server/server.js",
    "cypress:run": "npx cypress run",
    "cypress:open": "npx cypress open",
    "prepare": "husky install",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/router-store": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "lorem-ipsum": "^2.0.8",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@ngrx/eslint-plugin": "^14.3.2",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "cypress": "^10.9.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.3.0",
    "json-server": "^0.17.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "prettier-plugin-multiline-arrays": "^1.1.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.2"
  }
}